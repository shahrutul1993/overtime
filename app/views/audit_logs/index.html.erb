

<h1>Audit Log Dashbaoard</h1>
<table class="table table-hover table-striped">
	<thead>
		<tr>
			<th>
				#
			</th>
			<th>
				Employee
			</th>
			<th>
				Week starting
			</th>
			<th>
				Confirmed At
			</th>
			<th>
				Status
			</th>
		</tr>
	</thead>
	<tbody id="audit_logs">
		<%= render @audit_logs%>
	</tbody>
</table>

<div id="paginator">

<%= paginate @audit_logs, remote: true  %>
</div>
